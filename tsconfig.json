{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true
  },
  "include": ["js-bindings/**/*", "index.ts"],
  "exclude": ["dist/", "node_modules", "js-test/", "vitest.workspace.ts"]
}
